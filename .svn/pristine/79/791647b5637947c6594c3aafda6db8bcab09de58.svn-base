<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:theme="@style/dialog_style"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    
    <View
       android:background="@color/colorPrimaryDark"
       android:layout_width="0dp"
       android:layout_height="0dp"
       android:layout_alignLeft="@+id/actualcontent"
       android:layout_alignRight="@id/actualcontent"
       android:layout_alignTop="@id/actualcontent"
       android:layout_alignBottom="@id/actualcontent"/>
    
	<LinearLayout 
	    android:id="@+id/actualcontent"
	    android:orientation="vertical"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:paddingTop="@dimen/dialog_horizontal_padding"
	    android:paddingBottom="@dimen/dialog_horizontal_padding"
	    android:paddingLeft="@dimen/dialog_vertical_padding"
	    android:paddingRight="@dimen/dialog_vertical_padding">
	    
	            <ImageButton
		  	    android:id="@+id/dlg_btnBack"
		    	android:background="@drawable/layout_listitem"
                android:layout_marginBottom="@dimen/activity_horizontal_margin"
		      	android:src="@drawable/n_icoback"
				android:tint="@color/Black"
		      	android:layout_width="@dimen/back_ico"
		      	android:layout_height="@dimen/back_ico"
		      	android:scaleType="centerInside"
		      	android:layout_gravity="left"/>    
	    
	
	
            <ImageButton
                android:id="@+id/dlg_STP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@null"
                android:enabled="false"
                android:scaleType="fitXY"
                android:src="@drawable/btn_jstp" />
            
           <ImageButton
                android:id="@+id/dlg_POB"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@null"
                android:enabled="false"
                android:scaleType="fitXY"
                android:src="@drawable/btn_jpob" />
	
 
	        

	
	</LinearLayout>
</RelativeLayout>